	Month = {Aug},
	Number = {2},
	Pages = {111-6},
	Pmid = {968443},
	Pst = {ppublish},
	Title = {Plasma iron and erythrocytic glutathione peroxidase activity. A possible mechanism for oxidative haemolysis in iron deficiency anemia},
	Volume = {17},
	Year = {1976}}

